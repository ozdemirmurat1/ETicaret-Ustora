@using Eticaret.Entities
@model List<ProductImagesCustomModel>
<div class="maincontent-area">
    <div class="zigzag-bottom"></div>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="latest-product">
                    <h2 class="section-title">Latest Products</h2>
                    <div class="product-carousel">

                        @foreach (var item in Model)
                        {
                            <div class="single-product">
                                <div class="product-f-image">
                                    <img src="/product/@item.Image" alt="">
                                    <div class="product-hover">
                                        <a class="add-to-cart-link" onclick="AddBasket(1,@item.Id)"><i class="fa fa-shopping-cart"></i> Add to cart</a>
                                        <a href="/urun/@item.SeoName" class="view-details-link"><i class="fa fa-link"></i> See details</a>
                                    </div>
                                </div>

                                <h2><a href="/urun/@item.SeoName">@item.Name</a></h2>

                                <div class="product-carousel-price">
                                    <ins>@item.Price.ToString()</ins> <del>$1.0</del>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div> <!-- End main content area -->